<?xml version="1.0" ?>
<pattern>
   <title>Chromatic Splendor</title>
   <partnerID>LuxAstram</partnerID>
   <imageURL>images/WindowPatterns/ChromaticSplendor.jpg</imageURL>
   <rows>4</rows>
   <cols>5</cols>
   <difficulty>4</difficulty>
   <constraint col="2" color="GREEN" row="0" value="0"/>
   <constraint col="0" color="NOCOLOR" row="1" value="2"/>
   <constraint col="1" color="YELLOW" row="1" value="0"/>
   <constraint col="2" color="NOCOLOR" row="1" value="5"/>
   <constraint col="3" color="BLUE" row="1" value="0"/>
   <constraint col="4" color="NOCOLOR" row="1" value="1"/>
   <constraint col="1" color="RED" row="2" value="0"/>
   <constraint col="2" color="NOCOLOR" row="2" value="3"/>
   <constraint col="3" color="PURPLE" row="2" value="0"/>
   <constraint col="0" color="NOCOLOR" row="3" value="1"/>
   <constraint col="2" color="NOCOLOR" row="3" value="6"/>
   <constraint col="4" color="NOCOLOR" row="3" value="4"/>
</pattern>
